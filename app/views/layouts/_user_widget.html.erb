<% if user_signed_in? %>
    <font color="FFFFFF">You are logged in as <%= current_user.username %>   </font>
    <%= link_to 'Logout', destroy_user_session_path, :method => :delete, class:"btn btn-danger", :style=>'color:#FFFFFF;'%>
    <%= link_to 'Edit Profile', edit_registration_path(current_user), class:"btn btn-primary", :style=>'color:#FFFFFF;'%>
<% else %>
    <%= link_to "Log in", new_user_session_path, class:"btn btn-success", :style=>'color:#FFFFFF;' %>
    <%= link_to "Register", new_registration_path(resource_name), class:"btn btn-primary", :style=>'color:#FFFFFF;' %>
<% end %>
